    Sub AddInsert(ByVal quantity As String, ByVal price As Double)
        If decision = " ON SIDE" And ListBox2.SelectedIndex = -1 And quantity = "" Then
            ListBox1.Items.Add("")
            ListBox2.Items.Add(condament & decision)
            ListBox3.Items.Add(FormatNumber(price))
        ElseIf decision <> " ON SIDE" And ListBox2.SelectedIndex = -1 And quantity = "" Then
            ListBox1.Items.Add("")
            ListBox2.Items.Add(decision & condament)
            ListBox3.Items.Add(FormatNumber(price))
        ElseIf decision = " ON SIDE" And ListBox2.SelectedIndex = -1 And quantity <> "" Then
            ListBox1.Items.Add("")
            ListBox2.Items.Add(quantity & " " & condament & decision)
            ListBox3.Items.Add(FormatNumber(price * quantity))
        ElseIf decision <> " ON SIDE" And ListBox2.SelectedIndex = -1 And quantity <> "" Then
            ListBox1.Items.Add("")
            ListBox2.Items.Add(quantity & " " & decision & condament)
            ListBox3.Items.Add(FormatNumber(price * quantity))
        ElseIf decision = " ON SIDE" And ListBox2.SelectedIndex <> -1 And quantity = "" Then
            ListBox1.Items.Insert(ListBox2.SelectedIndex + 1, "") 'Insert item under the current selected item
            ListBox2.Items.Insert(ListBox2.SelectedIndex + 1, condament & decision)
            ListBox3.Items.Insert(ListBox2.SelectedIndex + 1, FormatNumber(price))
        ElseIf decision <> " ON SIDE" And ListBox2.SelectedIndex <> -1 And quantity = "" Then
            ListBox1.Items.Insert(ListBox2.SelectedIndex + 1, "") 'Insert item under the current selected item
            ListBox2.Items.Insert(ListBox2.SelectedIndex + 1, decision & condament)
            ListBox3.Items.Insert(ListBox2.SelectedIndex + 1, FormatNumber(price))
        End If
    End Sub
