    Sub OrderTotal()
        Dim total1 As Double = 0    'variable for total of the prices in listbox3
        Dim total2 As Double = 0    'holds the total of the prices and tax
        Dim tax As Double = 0       'holds the tax amount
        For i As Double = 0 To ListBox3.Items.Count - 1
            total1 += CDbl(ListBox3.Items(i))
            tax = total1 * 0.09
            total2 = total1 + tax
            TextBox1.Text = ""          'reset textbox1 to empty string if input was entered
            ViewTotal()
            lblTax2.Text = FormatNumber(tax, 2)
            lblPrice.Text = FormatNumber(total2, 2)
        Next
    End Sub
