    Sub AddInsertSizeSide(ByVal quantity, ByVal name, ByVal price)
        If quantity = "" Then
            ListBox1.Items.Add("")
            ListBox2.Items.Add(name)
            ListBox3.Items.Add(FormatNumber(price))
        ElseIf quantity <> "" Then
            ListBox1.Items.Add("")
            ListBox2.Items.Add(quantity & " " & name)
            ListBox3.Items.Add(FormatNumber(price * quantity))
        End If
        ListBox1.SelectedIndex = -1
        ListBox2.SelectedIndex = -1
        ListBox3.SelectedIndex = -1
    End Sub
    Dim sizze As String
    Dim sizze2 As String
    Dim DrinkSize As String = "SMALL "
    Sub AddInsertDrink(ByVal quantity, ByVal DrinkSize, ByVal name, ByVal price)
        If DrinkSize = "SMALL " Then
            price = 1.39
        ElseIf DrinkSize = "VALUE " Then
            price = 0.99
        ElseIf DrinkSize = "MEDIUM " Then
            price = 1.69
        ElseIf DrinkSize = "LARGE " Then
            price = 1.99
        ElseIf DrinkSize = "KIDS " Then
            price = 0.79
        End If
        If TextBox1.Text = "" Then
            quantity = 1
        Else
            quantity = TextBox1.Text
        End If
        ListBox1.Items.Add(quantity)
        ListBox2.Items.Add(DrinkSize & name)
        ListBox3.Items.Add(FormatNumber(price * quantity))
        ListBox1.SelectedIndex = -1
        ListBox2.SelectedIndex = -1
        ListBox3.SelectedIndex = -1
    End Sub
    Sub AddInsertDrinkCombo(ByVal quantity, ByVal sizze, ByVal name, ByVal price)
        If quantity = "" Then
            ListBox1.Items.Add("")
            ListBox2.Items.Add(sizze & name)
            ListBox3.Items.Add(FormatNumber(price))
        ElseIf quantity <> "" Then
            ListBox1.Items.Add("")
            ListBox2.Items.Add(quantity & " " & sizze & name)
            ListBox3.Items.Add(FormatNumber(price))
        End If
        ListBox1.SelectedIndex = -1
        ListBox2.SelectedIndex = -1
        ListBox3.SelectedIndex = -1
    End Sub
