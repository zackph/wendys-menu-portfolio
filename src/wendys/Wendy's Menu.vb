        If lblTend.Text = "Tend" Then
            MessageBox.Show("NO ORDERS WAITING", "Wand")
        ElseIf lblTend.Text = "Total" Then
            For i As Integer = 0 To ListBox4.Items.Count - 1
                ListBox1.Items.Add(ListBox4.Items(i))
            Next
            For i As Integer = 0 To ListBox5.Items.Count - 1
                ListBox2.Items.Add(ListBox5.Items(i))
            Next
            For i As Integer = 0 To ListBox6.Items.Count - 1
                ListBox3.Items.Add(ListBox6.Items(i))
            Next
            PaymentTrue()
            BurgersFalse()
            SubMenuFalse()
            CondamentsFalse()
            MenuFalse()
            ChickenFalse()
            ClearList2()
            ViewTotal()
            ViewTotal2False()
        End If
    End Sub

